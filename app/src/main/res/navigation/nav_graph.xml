            android:id="@+id/action_ComponentList_to_TimePicker"
            app:destination="@id/TimePickerScreen" />
        <action
            android:id="@+id/action_ComponentList_to_TopAppBar"
            app:destination="@id/TopAppBarTypeScreen" />
