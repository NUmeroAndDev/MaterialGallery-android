    <fragment
        android:id="@+id/ActionBarScreen"
        android:name="com.numero.material_gallery.components.appbar.top.ActionBarFragment"
        android:label="@string/actionbar"
        tools:layout="@layout/fragment_action_bar" />
    <fragment
        android:id="@+id/CollapsingScreen"
        android:name="com.numero.material_gallery.components.appbar.top.CollapsingFragment"
        android:label="@string/collapsing"
        tools:layout="@layout/fragment_collapsing" />
    <fragment
        android:id="@+id/ToolbarScreen"
        android:name="com.numero.material_gallery.components.appbar.top.ToolbarFragment"
        android:label="@string/toolbar"
        tools:layout="@layout/fragment_toolbar" />

