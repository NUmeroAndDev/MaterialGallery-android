        <action
            android:id="@+id/action_ComponentList_to_ProgressIndicator"
            app:destination="@id/ProgressIndicatorScreen" />
        <action
            android:id="@+id/action_ComponentList_to_Checkbox"
            app:destination="@id/CheckboxScreen" />
        <action
            android:id="@+id/action_ComponentList_to_RadioButton"
            app:destination="@id/RadioButtonScreen" />
        <action
            android:id="@+id/action_ComponentList_to_ModalBottomSheet"
            app:destination="@id/ModalBottomSheetScreen" />
